{
    "name": "laminas/laminas-inputfilter",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "inputfilter"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-inputfilter/",
        "issues": "https://github.com/laminas/laminas-inputfilter/issues",
        "source": "https://github.com/laminas/laminas-inputfilter",
        "rss": "https://github.com/laminas/laminas-inputfilter/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev",
            "dev-develop": "2.3-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "laminas/laminas-filter": "self.version",
        "laminas/laminas-stdlib": "self.version",
        "laminas/laminas-validator": "self.version",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-servicemanager": "self.version",
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "laminas/laminas-servicemanager": "To support plugin manager support"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\InputFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\InputFilter\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-inputfilter": "self.version"
    }
}
